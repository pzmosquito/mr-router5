{
  "name": "mr-router5",
  "version": "7.0.0-beta.1",
  "description": "mr-router5 uses Router5 together with MobX for your favorite React application.",
  "keywords": [
    "router5",
    "react",
    "mobx",
    "router",
    "routing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pzmosquito/mr-router5.git"
  },
  "author": "pzmosquito",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pzmosquito/mr-router5/issues"
  },
  "homepage": "https://github.com/pzmosquito/mr-router5#readme",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest --coverage --no-cache",
    "test:watch": "npm run test -- --watchAll",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "mobx": "^6",
    "mobx-react-lite": "^3 || ^4",
    "react": ">=16.8",
    "router5": "^7 || ^8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mobx": "^6.11.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "router5": "^8.0.1",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "router5-transition-path": "^8.0.1"
  }
}
